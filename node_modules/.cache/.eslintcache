nent/Header.js",[],[],"/home/insta-18/Desktop/RectJS/use-state/src/MyComponent/Edit.js",["60","61","62"],[],"import \"../css/Edit.css\"\nimport Stack from '@mui/material/Stack';\nimport Button from '@mui/material/Button';\nimport bold from '../Images/bold.png'\nimport underline from \"../Images/underline.png\"\nimport italic from \"../Images/italic.png\"\nimport ReactSwitch from 'react-switch';\nexport function Edit({onUnderline,onBold,onItalic,onhandleChange,toggle})\n{\n    return(\n        <div className=\"edit_container\">\n            <Stack spacing={2} direction=\"row\">\n            <Button variant=\"text\" ><img src={underline} height={20} width={20} onClick={onUnderline}/></Button>\n            <Button variant=\"contained\"><img src={bold} height={20} width={20} onClick={onBold}/></Button>\n            <Button variant=\"outlined\">I<img src={italic} height={20} width={20} onClick={onItalic}/></Button>\n            <div>\n                <ReactSwitch className=\"react-switch\" checked={toggle}  onChange={onhandleChange}  color=\"primary\" id=\"rs\"/>\n            </div>\n        </Stack>\n        </div>\n    );\n}","/home/insta-18/Desktop/RectJS/use-state/src/MyComponent/About.jsx",[],[],"/home/insta-18/Desktop/RectJS/use-state/src/MyComponent/Convert.jsx",["63"],[],"import \"../css/convert.css\";\nimport { useEffect } from \"react\";\nimport { useState } from \"react\";\nimport CardConvert from \"./CardConvert\";\nfunction Convert() {\n  let [Dollarmoney, setDollarMoney] = useState(0);\n  let [Rupeesmoney, setRupeesMoney] = useState(0);\n  let [celcius, setCelcius] = useState(32);\n  let [fahrenheit, setFahrenheit] = useState(0);\n  function convertDollar() {\n    let dollar = document.getElementById(\"dollar\").value;\n    setDollarMoney(82.44 * dollar);\n  }\n\n  function convertRupees() {\n    let rupees = document.getElementById(\"rupees\").value;\n    setRupeesMoney(rupees/82.44);\n  }\n\n  function convertCelcuis()\n  {\n    let celcius=document.getElementById(\"celcius\").value;\n    let c = (9/5 * celcius) + 32\n    setCelcius(c);\n  }\n\n  function convertFahrenheit()\n  {\n    let fahrenheit=document.getElementById(\"fahrenheit\").value;\n    let f = (fahrenheit-32) * (5/9)\n    setFahrenheit(f);\n  }\n  return (\n    <div>\n      <div>\n        <div className=\"input-group\">\n          <label className=\"lb\">Enter Money In $ To convert in ₹</label>\n          <span className=\"input-group-text sm\">$</span>\n          <input\n            id=\"dollar\"\n            type=\"text\"\n            class=\"form-control\"\n            aria-label=\"Amount (to the nearest dollar)\"\n            placeholder=\"0\"\n          />\n          <button type=\"button\" class=\"btn btn-primary\" onClick={convertDollar}>\n            Convert\n          </button>\n          <span className=\"money_container\">{Dollarmoney} ₹</span>\n        </div>\n      </div>\n\n      <div>\n        <div className=\"input-group\">\n          <label className=\"lb\">Enter Money In ₹ To convert in $</label>\n          <span className=\"input-group-text sm\">$</span>\n          <input\n          placeholder=\"0\"\n            id=\"rupees\"\n            type=\"text\"\n            class=\"form-control\"\n            aria-label=\"Amount (to the nearest dollar)\"\n          />\n          <button type=\"button\" class=\"btn btn-primary\" onClick={convertRupees}>\n            Convert\n          </button>\n          <span className=\"money_container\">{Rupeesmoney} $</span>\n        </div>\n      </div>\n\n      <div>\n        <div className=\"input-group\">\n          <label className=\"lb\">Enter temprature In C To convert in F</label>\n          <span className=\"input-group-text sm\">℃</span>\n          <input\n          placeholder=\"0\"\n            id=\"celcius\"\n            type=\"text\"\n            class=\"form-control\"\n            aria-label=\"Amount (to the nearest dollar)\"\n          />\n          <button type=\"button\" class=\"btn btn-primary\" onClick={convertCelcuis}>\n            Convert\n          </button>\n          <span className=\"money_container\">{celcius} ℉</span>\n        </div>\n      </div>\n\n      <div>\n        <div className=\"input-group\">\n          <label className=\"lb\">Enter Temprature In F To convert in C</label>\n          <span className=\"input-group-text sm\">℉</span>\n          <input\n          placeholder=\"0\"\n            id=\"fahrenheit\"\n            type=\"text\"\n            class=\"form-control\"\n            aria-label=\"Amount (to the nearest dollar)\"\n          />\n          <button type=\"button\" class=\"btn btn-primary\" onClick={convertFahrenheit}>\n            Convert\n          </button>\n          <span className=\"money_container\">{fahrenheit} ℃</span>\n        </div>\n      </div>\n      <div>\n        <CardConvert/>\n      </div>\n    </div>\n  );\n}\nexport default Convert;\n","/home/insta-18/Desktop/RectJS/use-state/src/MyComponent/Footer.js",[],[],"/home/insta-18/Desktop/RectJS/use-state/src/MyComponent/CardConvert.jsx",["64"],[],{"ruleId":"65","severity":1,"message":"66","line":13,"column":37,"nodeType":"67","endLine":13,"endColumn":104},{"ruleId":"65","severity":1,"message":"66","line":14,"column":41,"nodeType":"67","endLine":14,"endColumn":98},{"ruleId":"65","severity":1,"message":"66","line":15,"column":41,"nodeType":"67","endLine":15,"endColumn":102},{"ruleId":"68","severity":1,"message":"69","line":2,"column":10,"nodeType":"70","messageId":"71","endLine":2,"endColumn":19},{"ruleId":"68","severity":1,"message":"72","line":11,"column":10,"nodeType":"70","messageId":"71","endLine":11,"endColumn":17},"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'shadows' is defined but never used."]